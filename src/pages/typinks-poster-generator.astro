---
import Layout from '../layouts/Layout.astro'
import { Navbar } from '../components/Navbar.jsx'
import { ScrollUpButton } from '../components/ScrollUpButton.jsx'
import '../styles/Theme.css'
import Typinks from '../components/typinks/Typinks'
import Footer from '@/components/Footer.vue'
---

<Layout title="Typinks Poster Generator Online" description="Create custom posters for observance days with our easy-to-use generator. Add your own text and images to design meaningful visuals for any occasion!" ogImage='typinks.jpg'>
